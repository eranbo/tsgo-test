{
    "compilerOptions": {
        "module": "commonjs",
        "target": "ES2023",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "declaration": true,
        "declarationMap": true,
        "composite": true,
        "forceConsistentCasingInFileNames": true,
        "experimentalDecorators": true,
        "noImplicitThis": true,
        "strictBindCallApply": true,
        "skipLibCheck": true
    }
}
